{
    "name": "richcongress/bundle-toolbox",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A set of classes and tools to quickly build a bundle",
    "require": {
        "php": "^8.1",
        "symfony/http-kernel": "^6.0 || ^7.0",
        "symfony/config": "^6.0 || ^7.0",
        "symfony/dependency-injection": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "doctrine/doctrine-bundle": "^2.6",
        "doctrine/doctrine-migrations-bundle": "^2.0 || ^3.0"
    },
    "authors": [
        {
            "name": "RichCongress",
            "homepage": "https://www.richcongress.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RichCongress\\BundleToolbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RichCongress\\BundleToolbox\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
