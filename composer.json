{
    "name": "richcongress/bundle-toolbox",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "A set of classes and tools to quickly build a bundle",
    "require": {
        "php": ">=7.1",
        "symfony/http-kernel": "^3.0 || ^4.0 || ^5.0",
        "symfony/config": "^3.0 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "roave/security-advisories": "dev-master"
    },
    "authors": [
        {
            "name": "RichCongress",
            "homepage": "https://www.richcongress.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RichCongress\\BundleToolbox\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\RichCongress\\BundleToolbox\\": "Tests/"
        }
    }
}
